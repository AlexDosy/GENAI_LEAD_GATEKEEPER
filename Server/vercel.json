{
    "version": 2,
    "builds": [
      {
        "src": "Client/package.json",
        "use": "@vercel/angular"
      },
      {
        "src": "Server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "Client/dist/index.html"
      }
    ]
  }